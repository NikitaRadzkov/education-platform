{
   "plugins": [
      ["remark-gfm", true],
      ["remark-preset-lint-recommended", true],
      ["remark-lint-blockquote-indentation", 2],
      ["remark-lint-code-block-style", "fenced"],
      ["remark-lint-emphasis-marker", "*"],
      ["remark-lint-fenced-code-flag", { "allowEmpty": false }],
      ["remark-lint-fenced-code-marker", "`"],
      ["remark-lint-final-newline", true],
      ["remark-lint-first-heading-level", 1],
      ["remark-lint-heading-increment", true],
      ["remark-lint-heading-style", "atx"],
      ["remark-lint-linebreak-style", "consistent"],
      ["remark-lint-link-title-style", "\""],
      ["remark-lint-list-item-bullet-indent", true],
      ["remark-lint-list-item-content-indent", true],
      ["remark-lint-list-item-indent", "space"],
      ["remark-lint-list-item-spacing", {"checkBlanks": true}],
      ["remark-lint-maximum-heading-length", 80],
      ["remark-lint-maximum-line-length", 120],
      ["remark-lint-no-auto-link-without-protocol", true],
      ["remark-lint-no-blockquote-without-marker", true],
      ["remark-lint-no-consecutive-blank-lines", true],
      ["remark-lint-no-duplicate-defined-urls", true],
      ["remark-lint-no-duplicate-definitions", true],
      ["remark-lint-no-duplicate-headings", false],
      ["remark-lint-no-duplicate-headings-in-section", true],
      ["remark-lint-no-emphasis-as-heading", true],
      ["remark-lint-no-empty-url", true],
      ["remark-lint-no-file-name-articles", true],
      ["remark-lint-no-file-name-consecutive-dashes", true],
      ["remark-lint-no-file-name-irregular-characters", true],
      ["remark-lint-no-file-name-mixed-case", true],
      ["remark-lint-no-file-name-outer-dashes", true],
      ["remark-lint-no-heading-content-indent", true],
      ["remark-lint-no-heading-indent", true],
      ["remark-lint-no-heading-like-paragraph", true],
      ["remark-lint-no-heading-punctuation", true],
      ["remark-lint-no-html", true],
      ["remark-lint-no-inline-padding", true],
      ["remark-lint-no-literal-urls", true],
      ["remark-lint-no-missing-blank-lines", { "exceptTightLists": true }],
      ["remark-lint-no-multiple-toplevel-headings", 1],
      ["remark-lint-no-paragraph-content-indent", true],
      ["remark-lint-no-reference-like-url", true],
      ["remark-lint-no-shell-dollars", true],
      ["remark-lint-no-shortcut-reference-image", true],
      ["remark-lint-no-shortcut-reference-link", true],
      ["remark-lint-no-table-indentation", true],
      ["remark-lint-no-tabs", true],
      ["remark-lint-no-undefined-references", true],
      ["remark-lint-ordered-list-marker-style", "."],
      ["remark-lint-ordered-list-marker-value", "ordered"],
      ["remark-lint-rule-style", "***"],
      ["remark-lint-strong-marker", false],
      ["remark-lint-table-cell-padding", "padded"],
      ["remark-lint-table-pipe-alignment", true],
      ["remark-lint-table-pipes", true],
      ["remark-lint-unordered-list-marker-style", "*"],
      ["remark-lint-no-empty-sections", true],
      ["remark-lint-no-trailing-spaces", true],
      ["remark-lint-no-dead-urls", {"skipOffline": true, "skipUrlPatterns": ["https://developer.android.com", "https://source.android.com", "https://codelabs.developers.google.com", "https://swagger.io", "https://www.discogs.com", "https://www.postman.com", "https://www.softwaretestinghelp.com", "https://chrome.google.com", "https://www.telerik.com", "https://learnappmaking.com", "https://yoichitgy.github.io", "https://www.raywenderlich.com", "https://academy.realm.io", "https://www.codementor.io"]}],
      ["remark-validate-links", true]
   ]
}
